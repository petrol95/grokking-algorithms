<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://www.example.com/" xmlns:tns="http://www.example.com/">
    <element name="students">
        <complexType>
            <sequence>
                <element name="student" type="tns:student" minOccurs="2" maxOccurs="unbounded" />
            </sequence>
        </complexType>
    </element>
    <complexType name="student">
        <sequence>
            <element name="name" type="string" />
            <element name="telephone" type="positiveInteger" />
            <element name="address" type="tns:address" />
        </sequence>
        <attribute name="login" type="tns:login" use="required" />
        <attribute name="faculty" use="optional" default="mmf">
            <simpleType>
                <restriction base="string">
                    <enumeration value="mmf"></enumeration>
                    <enumeration value="geo"></enumeration>
                    <enumeration value="ksis"></enumeration>
                </restriction>
            </simpleType>
        </attribute>
    </complexType>
    <simpleType name="login">
        <restriction base="ID">
            <pattern value="([a-zA-Z])[a-zA-Z0-9]{7,19}" />
        </restriction>
    </simpleType>
    <complexType name="address">
        <sequence>
            <element name="country" type="string" />
            <element name="city" type="string" />
            <element name="street" type="string" />
        </sequence>
    </complexType>
</schema>